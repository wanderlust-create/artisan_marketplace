<h1>Edit Artisan Details</h1>

<%= form_with model: [@admin, @artisan], local: true do |form| %>
  <div>
    <%= form.label :store_name, "Store Name" %>
      <%= form.text_field :store_name %>
  </div>
  <div>
    <%= form.label :email, "Email" %>
      <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password, "Password (leave blank if unchanged)" %>
      <%= form.password_field :password, autocomplete: "new-password" %>
  </div>
  <div>
    <%= form.label :password_confirmation, "Confirm Password" %>
      <%= form.password_field :password_confirmation %>
  </div>
  <div>
    <%= form.submit "Update Artisan" %>
  </div>
  <% end %>

    <%= link_to 'Back to Dashboard', dashboard_path_for(current_user) %>